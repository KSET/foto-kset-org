= simple_form_for application, remote: true do |f|
  = f.input :first_name, label: 'Ime'
  = f.input :last_name, label: 'Prezime'
  = f.input :email
  = f.input :mobile_phone, label: 'Broj mobitela'
  .hidden
    = f.association :season, input_html: { hidden: true }, selected: season.id, label: false
